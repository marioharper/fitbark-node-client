{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "arrow-body-style": "off",
        "import/no-extraneous-dependencies": ["error", {
            "devDependencies": ["gulpfile.js","**/*.test.js", "build/**/*.js", "**/*.spec.js"]
        }]
    },
    "env": {
        "node": true
    }
}
